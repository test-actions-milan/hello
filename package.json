{
  "name": "clinician-portal",
  "version": "6.14.1",
  "private": true,
  "scripts": {
    "start": "PORT=3001 env-cmd -f .env vite",
    "start:dev": "PORT=3001 env-cmd -f .env.development vite",
    "build": "vite build",
    "test": "CI=true jest --config jest.config.json --watchAll --colors --verbose --coverage --forceExit",
    "test:unit": "jest --config jest.config.json --watchAll --colors",
    "test:ci": "CI=true jest --config jest.config.json --verbose --colors --coverage --json --outputFile=coverage/coverage-final.json --forceExit",
    "test:watch": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage --verbose --forceExit",
    "prettier": "prettier --list-different \"{src,api,public}/**/*.{js,jsx,html,css,md,yml,yml,json}\"",
    "prettier:fix": "prettier --write \"{src,api,public}/**/*.{js,jsx,html,css,md,yml,yml,json}\" --log-level warn",
    "eslint": "eslint \"{src,api,public}/**/*.{js,jsx}\"",
    "eslint:fix": "eslint --fix \"{src,api,public}/**/*.{js,jsx}\"",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "bootstrap": "echo 'Removing node_modules...' && rm -rf node_modules && npm i",
    "prepare": "husky",
    "pretest": "node cypress/support/setUpTestData.js",
    "cy:open": "env-cmd -f .env.automation --silent npx cypress open",
    "cy:open_run": "env-cmd -f .env.automation --silent npx cypress run",
    "cy:delete_cucumber-json": "rm -rf cypress/cucumber-json",
    "cypress:tags_headed": "npm run cy:delete_cucumber-json && env-cmd -f .env.automation --silent npx cypress-tags run -g \"cypress/e2e/**/*.feature\" --headed -e",
    "cy:run_tags": "npm run cy:delete_cucumber-json && env-cmd -f .env.automation --silent npx cypress-tags run -g \"cypress/e2e/**/*.feature\" -e",
    "cy:run_report": "env-cmd -f .env.automation --silent npx cypress run && node cucumber-html-report.js",
    "cy:run_head_report": "npm run cy:delete_cucumber-json && env-cmd -f .env.automation --silent npx cypress run --headed && node cucumber-html-report.js",
    "cy:report": "node cucumber-html-report.mjs"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true
    },
    "stepDefinitions": "cypress/e2e/step_definitions"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
