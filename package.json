    "date-fns-tz": "^2.0.1",
    "dayjs": "^1.11.11",
    "deepdash": "^5.3.9",
    "detect-browser": "^5.0.0",
    "file-saver": "^2.0.5",
    "fuzzysort": "^2.0.4",
    "history": "^4.9.0",
    "intersection-observer": "^0.12.2",
    "izitoast": "^1.4.0",
    "libphonenumber-js": "^1.11.2",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.50.0",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "owasp-password-strength-test": "^1.3.0",
