    "react-idle-timer": "^4.6.4",
    "react-redux": "^8.1.3",
    "react-responsive": "^5.0.0",
    "react-router-dom": "^5.3.4",
    "react-sticky": "^6.0.2",
    "react-table": "^7.6.3",
    "react-to-print": "^2.15.1",
    "redux": "^5.0.1",
